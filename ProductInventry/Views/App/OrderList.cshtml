@model List<ProductInventry.Data.Entities.ListOrder>
@{
}
<head>
    <style>
        table, th, td {
            border: 1px solid black;
            border-collapse: collapse;
        }

        th, td {
            padding: 15px;
            text-align: left;
        }

        table#t01 {
            width: 100%;
            background-color: #f1f1c1;
        }
    </style>
</head>
<body>
   
    
        <table>
            <tr>
                <th>Order ID</th>
                <th>Total Price</th>
                <th>View Order</th>
            </tr>
            
            @foreach(var item in Model)
            {
                <tr>

                    <td>
                        @Html.DisplayFor(modelItem =>item.OrderID)
                    </td>
                    <td>

                        @Html.DisplayFor(modelItem => item.TotalPrice)
                    </td>
                    <td>
                        @Html.ActionLink("View","ShowOrder", "App",new {id=item.OrderID})
                    </td>

                </tr>

            }

        </table>
    
</body>